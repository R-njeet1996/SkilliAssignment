<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TextView">
        <attr name="ctypeface" format="string" />
        <attr name="android:text" />
        <attr name="cHintTypface" format="string" />
    </declare-styleable>

    <declare-styleable name="CircularProgressView">
        <attr format="integer" name="max"/>
        <attr format="boolean" name="shadow"/>
        <attr format="boolean" name="progressThumb"/>
        <attr format="integer" name="startingAngle"/>
        <attr format="integer" name="progress"/>
        <attr format="dimension" name="progressBarThickness"/>
        <attr format="color" name="progressBarColor"/>
        <attr format="color" name="backgroundColor"/>
        <attr format="reference" name="thumb_image"/>
    </declare-styleable>
    <declare-styleable name="FloatingIconLayout">
        <attr name="enter_duration" format="integer" />
        <attr name="duration" format="integer" />
        <attr name="scale" format="float" />
        <attr name="same_size" format="boolean" />
    </declare-styleable>

    <attr name="inputType" format="enum">
        <enum name="text" value="0"/>
        <enum name="number" value="1"/>
    </attr>


</resources>